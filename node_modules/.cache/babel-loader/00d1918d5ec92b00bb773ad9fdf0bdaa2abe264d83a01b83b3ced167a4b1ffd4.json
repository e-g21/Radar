{"ast":null,"code":"import { ref } from 'vue';\nimport { reactive } from 'vue'; // üëà Agrega esta l√≠nea\nimport { FwbSelect } from 'flowbite-vue';\nexport default {\n  __name: 'FichaPerfil',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const selected = ref('');\n    const countries = [{\n      value: 'us',\n      name: 'Comida'\n    }, {\n      value: 'ca',\n      name: 'Calzados'\n    }, {\n      value: 'fr',\n      name: 'Veh√≠culos'\n    }];\n    const agenciasTel = [{\n      value: '0412',\n      name: '0412'\n    }, {\n      value: '0424',\n      name: '0424'\n    }, {\n      value: '0414',\n      name: '0414'\n    }, {\n      value: '0416',\n      name: '0416'\n    }, {\n      value: '0426',\n      name: '0426'\n    }];\n    const horarios = ref([]);\n    const mostrarFormulario = ref(false);\n    const editIndex = ref(null);\n    const form = ref({\n      dias: \"\",\n      desde: \"\",\n      hasta: \"\"\n    });\n    const abrirFormulario = (index = null) => {\n      if (index !== null) {\n        // Editar\n        editIndex.value = index;\n        form.value = {\n          ...horarios.value[index]\n        };\n      } else {\n        // Nuevo\n        editIndex.value = null;\n        form.value = {\n          dias: \"\",\n          desde: \"\",\n          hasta: \"\"\n        };\n      }\n      mostrarFormulario.value = true;\n    };\n    const cerrarFormulario = () => {\n      mostrarFormulario.value = false;\n    };\n    const guardarHorario = () => {\n      if (editIndex.value !== null) {\n        horarios.value[editIndex.value] = {\n          ...form.value\n        };\n      } else {\n        horarios.value.push({\n          ...form.value\n        });\n      }\n      cerrarFormulario();\n    };\n    const eliminarHorario = index => {\n      horarios.value.splice(index, 1);\n    };\n    const __returned__ = {\n      selected,\n      countries,\n      agenciasTel,\n      horarios,\n      mostrarFormulario,\n      editIndex,\n      form,\n      abrirFormulario,\n      cerrarFormulario,\n      guardarHorario,\n      eliminarHorario,\n      ref,\n      reactive,\n      get FwbSelect() {\n        return FwbSelect;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","reactive","FwbSelect","selected","countries","value","name","agenciasTel","horarios","mostrarFormulario","editIndex","form","dias","desde","hasta","abrirFormulario","index","cerrarFormulario","guardarHorario","push","eliminarHorario","splice"],"sources":["C:/xampp/htdocs/radar/src/views/Dashboard/FichaPerfil.vue"],"sourcesContent":["<template>\r\n  <div class=\"f-perfil\">\r\n    <div class=\"content-banner\" id=\"f-banner\">\r\n      <div class=\"card flex-1 py-10 bg-gray-300 text-center\">\r\n        <h2 class=\"p-title\">Banner Comercial</h2>\r\n        <p class=\"p-text mb-10\">\r\n          Foto de portada del banner de tu perfil comercial\r\n        </p>\r\n        <div class=\"logo-comercial\">\r\n          <img src=\"@/assets/img/gs-logo.png\" alt=\"Logo\" class=\"w-full hidden md:block\" />\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"mt-10 w-full pt-10\">\r\n        <form action=\"#\" method=\"POST\" class=\"space-y-6 pt-10\">\r\n          <div class=\"grid grid-cols-2 gap-8 mt-19 w-full\">\r\n            <div class=\"c-perfil\">\r\n              <h4 class=\"p-title mb-4 text-lg\">Datos de Sucursal</h4>\r\n              <div class=\"c-dates\">\r\n                <label for=\"text\" class=\"label-input\">Nombre Comercial</label>\r\n                <div class=\"mt-2\">\r\n                  <input type=\"text\" name=\"text\" required placeholder=\"Andr√©s Torres\" class=\"t-input\" />\r\n                </div>\r\n              </div>\r\n              <div class=\"c-dates\">\r\n                <label for=\"text\" class=\"label-input\">Categor√≠as</label>\r\n                <div class=\"mt-2\">\r\n                  <fwb-select v-model=\"selected\" :options=\"countries\" />\r\n                </div>\r\n              </div>\r\n              <!--  -->\r\n              <div class=\"c-dates\">\r\n                <label for=\"text\" class=\"label-input\">Configurar Horarios</label>\r\n                <!-- Bot√≥n agregar -->\r\n                <input @click=\"abrirFormulario()\" type=\"text\" placeholder=\"Agregar Horario\" class=\"t-input\" />\r\n                <!-- Formulario (abre solo al presionar + M√°s o Editar) -->\r\n                <div v-if=\"mostrarFormulario\" class=\"mt-6 p-4 border rounded-lg bg-gray-50\">\r\n                  <!-- Select de d√≠as -->\r\n                  <label class=\"block text-sm font-medium mb-1\">D√≠as</label>\r\n                  <select v-model=\"form.dias\" class=\"w-full p-2 border rounded mb-3\">\r\n                    <option value=\"\">Seleccione d√≠as</option>\r\n                    <option value=\"lunes-viernes\">Lunes a Viernes</option>\r\n                    <option value=\"sabado-domingo\">S√°bado y Domingo</option>\r\n                    <option value=\"lunes\">Lunes</option>\r\n                    <option value=\"martes\">Martes</option>\r\n                    <option value=\"miercoles\">Mi√©rcoles</option>\r\n                    <option value=\"jueves\">Jueves</option>\r\n                    <option value=\"viernes\">Viernes</option>\r\n                    <option value=\"sabado\">S√°bado</option>\r\n                    <option value=\"domingo\">Domingo</option>\r\n                  </select>\r\n\r\n                  <!-- Horas -->\r\n                  <div class=\"grid grid-cols-2 gap-4\">\r\n                    <div>\r\n                      <label class=\"block text-sm font-medium mb-1\">Desde</label>\r\n                      <input type=\"time\" v-model=\"form.desde\" class=\"w-full p-2 border rounded\" />\r\n                    </div>\r\n                    <div>\r\n                      <label class=\"block text-sm font-medium mb-1\">Hasta</label>\r\n                      <input type=\"time\" v-model=\"form.hasta\" class=\"w-full p-2 border rounded\" />\r\n                    </div>\r\n                  </div>\r\n                  <!-- Botones -->\r\n                  <div class=\"flex justify-end space-x-3 mt-4\">\r\n                    <button @click=\"cerrarFormulario\" class=\"px-4 py-2 bg-gray-300 rounded hover:bg-gray-400\">\r\n                      Cancelar\r\n                    </button>\r\n                    <button @click=\"guardarHorario\" class=\"px-4 py-2 bg-pink-600 text-white rounded hover:bg-pink-700\">\r\n                      Guardar\r\n                    </button>\r\n                  </div>\r\n                </div>\r\n                <!-- Lista de horarios guardados -->\r\n                <div v-if=\"horarios.length > 0\" class=\"mt-6\">\r\n                  <h3 class=\"text-lg font-bold mb-2\">Horarios Configurados:</h3>\r\n                  <ul class=\"list-disc pl-5 space-y-2\">\r\n                    <li v-for=\"(horario, index) in horarios\" :key=\"index\"\r\n                      class=\"flex items-center justify-between bg-gray-50 p-2 rounded\">\r\n                      <span>\r\n                        <strong>{{ horario.dias }}</strong>: {{ horario.desde }} - {{ horario.hasta }}\r\n                      </span>\r\n                      <div class=\"space-x-3\">\r\n                        <button class=\"text-blue-600 hover:underline\" @click=\"abrirFormulario(index)\">\r\n                          Editar\r\n                        </button>\r\n                        <button class=\"text-red-600 hover:underline\" @click=\"eliminarHorario(index)\">\r\n                          Eliminar\r\n                        </button>\r\n                      </div>\r\n                    </li>\r\n                  </ul>\r\n                </div>\r\n              </div>\r\n              <!--  -->\r\n           