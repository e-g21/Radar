 Contactos,\r\n  CuentaPropietario,\r\n  TerminosCondiciones\r\n}\r\n\r\n\r\n// Estado actual\r\nconst vistaActualDashboard = ref('Bienvenido')\r\n\r\n// Estado del menú activo\r\nconst menuActivo = ref('menu-bienvenido') // 'menu-bienvenido' o 'menu-dashboard'\r\n\r\n// Control del sidebar en modo responsive\r\nconst showOffcanvas = ref(false)\r\n// Función para abrir/cerrar sidebar desde Navbar o Sidebar\r\nfunction toggleOffcanvas() {\r\n  showOffcanvas.value = !showOffcanvas.value\r\n}\r\n\r\n// Función para cambiar vista\r\nfunction cambiarVista(nuevaVista) {\r\n  if (vistas[nuevaVista]) {\r\n    vistaActualDashboard.value = nuevaVista\r\n  }\r\n}\r\n// Función para alternar entre menús y cambiar la vista inicial\r\nfunction toggleMenu() {\r\n  if (menuActivo.value === 'menu-dashboard') {\r\n    menuActivo.value = 'menu-bienvenido'\r\n    vistaActualDashboard.value = 'Bienvenido' // primer item del menú-bienvenido\r\n  } else {\r\n    menuActivo.value = 'menu-dashboard'\r\n    vistaActualDashboard.value = 'Panel' // primer item del menú-dashboard\r\n  }\r\n}\r\n</script>\r\n"],"mappings":";;EAEOA,KAAK,EAAC;AAAwB;;EAG5BA,KAAK,EAAC;AAAQ;;EAEZA,KAAK,EAAC;AAAgB;;EAUpBA,KAAK,EAAC;AAAc;;;;;uBAf/BC,mBAAA,CAsBM,OAtBNC,UAsBM,GArBJC,YAAA,CAA2CC,MAAA;IAAlCC,eAAa,EAAED,MAAA,CAAAE;EAAe,IACvCC,mBAAA,yBAA4B,EAC5BC,mBAAA,CAkBM,OAlBNC,UAkBM,GAjBJF,mBAAA,yBAA4B,EAC5BC,mBAAA,CAcM,OAdNE,UAcM,GAbHP,YAAA,CAODC,MAAA;IANCO,UAAU,EAAEP,MAAA,CAAAO,UAAU;IACtBC,WAAW,EAAER,MAAA,CAAAS,oBAAoB;IACjCC,aAAa,EAAEV,MAAA,CAAAU,aAAa;IAC5BC,iBAAgB,EAAEX,MAAA,CAAAE,eAAe;IACjCU,cAAa,EAAEZ,MAAA,CAAAa,YAAY;IAC3BC,YAAW,EAAEd,MAAA,CAAAe;2EAGdX,mBAAA,CAGM,OAHNY,UAGM,GAFahB,MAAA,CAAAiB,MAAM,CAACjB,MAAA,CAAAS,oBAAoB,K,cAA5CS,YAAA,CAA8GC,wBAAA,CAA1DnB,MAAA,CAAAiB,MAAM,CAACjB,MAAA,CAAAS,oBAAoB;;IAAIK,YAAW,EAAEd,MAAA,CAAAe;sDAChGlB,mBAAA,CAAqC,OAAAuB,UAAA,EAAzB,qBAAmB,G","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}